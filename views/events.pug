extends layout.pug

block header
	div(class="jumbotron event-jumbo p-3 p-md-5 text-white bg-dark")
		div(class="col-md-6 px-0")
			h1(class="display-4 font-bold") Events
			p(class="lead my-3") Multiple lines of text that form the lede, informing new readers quickly and efficiently about what's most interesting in this post's contents.
			p(class="lead mb-0")
				a(href="#" class="text-white font-weight-bold") See Full Calendar...

block content
	div(class="row")
		h3(class="display-3 font-italic mb-5") Upcoming Events ${dates}
	div(class="row")
		each val in dates
			div(class="col-md-3")
				div(class="card")
					div(class="jumbotron jumbotron-fluid p-md-3 text-white main-event-head" style="height: 100%; width: 100%;")
						h5(class="card-title text-center")= val.title
					div(class="card-body main-event-body")
						p(class="main-event-date") 
							span(class="main-event-month rockTxt")= val.month
							span(class="main-event-day")= val.day
						p(class="main-event-time")= val.time
						p(class="main-event-location") 
							a(href=val.location.link)= val.location.title 
						p(class="card-text")= val.desc
						p(class="card-text")= val.summary
						a(class="btn btn-carousel" href="#") View in Calendar

		div(class="col-md-3")
			div(class="card")
				div(class="jumbotron jumbotron-fluid p-md-3 text-white main-event-head" style="height: 100%; width: 100%;")
					h5(class="card-title text-center") üèà Clemson Tigers @ Texas A&M Aggies
				div(class="card-body main-event-body")
					p(class="main-event-date") 
						span(class="main-event-month rockTxt") Sept 
						span(class="main-event-day") 24
					p(class="main-event-time") 10:00pm - 12:00pm
					p(class="main-event-location") 
						a(href="#") Central Wharf Company
