doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
		meta(name="description" content="NECC Console")
		meta(name="author" content="Mike DeHart")
		title NECC Console

		// Favicon
		link(rel="icon" href="img/favicon.ico")

		// Bootstrap CSS
		link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous")

		// Custom CSS
		link(href="css/base.css" rel="stylesheet" type="text/css")

	body
		main(role="main" class="container")
			div(class="row")
				div(class="col-md-12 col-sm-12")
					h1(class="display-3 text-center") NECC Console

			div(class="row")
				div(class="col-md-6 col-sm-12")
					h2(class="display-4 text-center") File Upload
					form
						div(class="form-group")
							label(for="gTitle") Title
							input(type="text" class="form-control" id="titleInput" aria-describedby="titleHelp" placeholder="Gallery Title")

				div(class="col-md-6 col-sm-12")
					h2(class="display-4 text-center") Get members
					button(class="btn btn-clemson btn-block" type="button") Fetch
				

