doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
		meta(name="description" content="NECC Console")
		meta(name="author" content="Mike DeHart")
		title NECC Console

		// Favicon
		link(rel="icon" href="img/favicon.ico")

		// Bootstrap CSS
		link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous")

		// Custom CSS
		link(href="css/base.css" rel="stylesheet" type="text/css")

	body
		main(role="main")
			div(class="container marketing")
				div(class="row")
					h1(class="display-3 text-center") NECC Console

				div(class="row")
					div(class="row")
						h2(class="display-4") File Upload
						div(class="col-md-6")
							form
								label(class="sr-only" for="input")
								input(class="form-control" name="input" placeholder="testing")

				div(class="row")
					h2(class="display-4") Get members
					div(class="col-md-12")
						form
							label(class="sr-only" for="input")
							input(class="form-control" name="input" placeholder="testing")


